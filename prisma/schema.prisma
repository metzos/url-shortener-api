generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model UrlEntry {
  id          Int      @id @default(autoincrement())
  originalUrl String   
  shortCode   String   @unique
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  clicks      Int      @default(0)
}